{
    "version": 2,
    "functions": {
        "main.py": {
            "runtime": "@vercel/python"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "main.py"
        }
    ],
    "env": {
        "DATABASE_URL": "@database_url",
        "SECRET_KEY": "@secret_key",
        "ALGORITHM": "HS256",
        "ACCESS_TOKEN_EXPIRE_MINUTES": "30"
    }
}